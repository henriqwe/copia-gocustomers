{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "&/*": [
        "components/dominios/*"
      ],
      "&R/*": [
        "components/dominios/rastreamento/*"
      ],
      "&A/*": [
        "components/dominios/assistencia/*"
      ],
      "&M/*": [
        "components/dominios/maxline/*"
      ],
      "@/*": [
        "components/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
